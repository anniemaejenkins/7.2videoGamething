<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h1>My Video Game Collection!</h1>

    <form class="" action="/" method="post">
      <input type="text" name="title" placeholder="title" value="">
      <input type="tel" name="release[year]" placeholder="release year" value="">
      <input type="text" name="release[country]" placeholder="country" value="">
      <input type="submit" value="Add Game">
    </form>

    {{#model}}
    <h2>{{title}}</h2>
    <div class="">
      <h4>Released:</h4>
      <ul>
        {{#release}}
        <li>{{year}}</li>
        <li>{{country}}</li>
        {{/release}}
      </ul>
    </div>
    {{/model}}

  </body>
</html>
